  if (!text) return '';
  return text
    .replace(/^[#*\-\s\d\.]+/gm, '')           // –£–±–∏—Ä–∞–µ–º –º–∞—Ä–∫–µ—Ä—ã —Å–ø–∏—Å–∫–æ–≤ –∏ –∑–∞–≥–æ–ª–æ–≤–∫–∏
    .replace(/\*{1,2}(.*?)\*{1,2}/g, '$1')     // –£–±–∏—Ä–∞–µ–º **bold** –∏ *italic*
    .replace(/`{1,3}(.*?)`{1,3}/g, '$1')       // –£–±–∏—Ä–∞–µ–º `code` –∏ ```code blocks```
    .replace(/\n{3,}/g, '\n\n')                // –ù–æ—Ä–º–∞–ª–∏–∑—É–µ–º –ø–µ—Ä–µ–Ω–æ—Å—ã —Å—Ç—Ä–æ–∫
    .trim();
}

/**
 * –ü–æ–ª—É—á–∞–µ—Ç —Ç–µ–∫—É—â–∏–π –º–∞–∫—Ä–æ—ç–∫–æ–Ω–æ–º–∏—á–µ—Å–∫–∏–π –∫–æ–Ω—Ç–µ–∫—Å—Ç.
 * @returns {Object} –û–±—ä–µ–∫—Ç —Å –º–∞–∫—Ä–æ—ç–∫–æ–Ω–æ–º–∏—á–µ—Å–∫–∏–º–∏ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞–º–∏.
 */
function getMacroContext() {
  try {
    const settingsSheet = SPREADSHEET.getSheetByName('–ù–∞—Å—Ç—Ä–æ–π–∫–∏');
    if (settingsSheet) {
      const macroData = settingsSheet.getRange(MACRO_CONTEXT_CELL).getValue();
      if (macroData) {
        try {
          return JSON.parse(macroData);
        } catch (e) {
          // –ï—Å–ª–∏ –Ω–µ JSON, –ø—Ä–æ–±—É–µ–º –ø–∞—Ä—Å–∏—Ç—å –∫–∞–∫ —Å—Ç—Ä–æ–∫—É key=value
          Logger.log('–ú–∞–∫—Ä–æ–∫–æ–Ω—Ç–µ–∫—Å—Ç –Ω–µ –≤ JSON —Ñ–æ—Ä–º–∞—Ç–µ, –∏—Å–ø–æ–ª—å–∑—É–µ–º –∑–Ω–∞—á–µ–Ω–∏—è –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é');
        }
      }
    }
  } catch (e) {
    Logger.log(`–û—à–∏–±–∫–∞ –ø–æ–ª—É—á–µ–Ω–∏—è –º–∞–∫—Ä–æ–∫–æ–Ω—Ç–µ–∫—Å—Ç–∞: ${e.message}`);
  }
  return DEFAULT_MACRO_CONTEXT;
}