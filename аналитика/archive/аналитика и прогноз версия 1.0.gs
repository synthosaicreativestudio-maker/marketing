      Logger.log('–õ–∏—Å—Ç "–∫—Ä–∏—Ç–µ—Ä–∏–∏ –¥–ª—è —Å—Ä–∞–≤–Ω–µ–Ω–∏—è –∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–æ–≤" —Å–æ–∑–¥–∞–Ω');
    }
    
    // –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –ª–∏—Å—Ç
    sheet.clear();
    
    // –ó–∞–≥–æ–ª–æ–≤–∫–∏
    const headers = [
      ['–ö–†–ò–¢–ï–†–ò–ò –î–õ–Ø –ü–û–ò–°–ö–ê –ö–û–ù–ö–£–†–ï–ù–¢–û–í', ''],
      ['', ''],
      ['–ü–ê–†–ê–ú–ï–¢–†–´ –û–ë–™–ï–ö–¢–ê', '–ó–ù–ê–ß–ï–ù–ò–ï'],
      ['–ö–æ–¥ –æ–±—ä–µ–∫—Ç–∞', ''],
      ['–†–∞–π–æ–Ω', ''],
      ['–¢–∏–ø –æ–±—ä–µ–∫—Ç–∞', ''],
      ['–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–æ–º–Ω–∞—Ç', ''],
