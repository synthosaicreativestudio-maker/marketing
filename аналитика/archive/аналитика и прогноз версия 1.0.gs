    ['vs —Ä–µ–∞–ª—å–Ω—ã–µ —Å–¥–µ–ª–∫–∏', `${analysisResults.marketRatios.soldRatio > 0 ? '+' : ''}${analysisResults.marketRatios.soldRatio.toFixed(1)}%`],
    ['–°—Ä. —Ü–µ–Ω–∞ –∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–æ–≤', `${Math.round(analysisResults.marketRatios.avgActivePrice * Number(objectData[4])).toLocaleString()} ‚ÇΩ`],
    ['–°—Ä. —Ü–µ–Ω–∞ —Å–¥–µ–ª–æ–∫', `${Math.round(analysisResults.marketRatios.avgSoldPrice * Number(objectData[4])).toLocaleString()} ‚ÇΩ`],
    ['', ''],
    ['üí∞ –†–ï–ö–û–ú–ï–ù–î–ê–¶–ò–Ø', ''],
    ['–ë—ã—Å—Ç—Ä–æ (1-2 –º–µ—Å)', `${analysisResults.forecasts.quickPrice.toLocaleString()} ‚ÇΩ`],
    ['–†—ã–Ω–æ–∫ (3-4 –º–µ—Å)', `${analysisResults.forecasts.marketPrice.toLocaleString()} ‚ÇΩ`],
    ['', ''],
    ['‚è±Ô∏è –°–†–û–ö–ò', ''],
    ['–°–µ–π—á–∞—Å', `${monthNames[currentMonth]}`],
    ['–°–µ–∑–æ–Ω', `${seasonalCoef > 1 ? '–¥–æ–ª—å—à–µ –Ω–∞ ' : '–±—ã—Å—Ç—Ä–µ–µ –Ω–∞ '}${Math.abs(Math.round((seasonalCoef - 1) * 100))}%`],
    ['–ü—Ä–æ–≥–Ω–æ–∑', `${dynamicResult.months.toFixed(1)} –º–µ—Å.`],
    ['–ü—Ä–∏–º–µ—á–∞–Ω–∏–µ', dynamicResult.info],
    ['', ''],
    ['üìà –≠–ö–û–ù–û–ú–ò–ö–ê', ''],
    ['–°—Ç–∞–≤–∫–∞ –¶–ë', `${macro.keyRate}%`],
    ['–ò–ø–æ—Ç–µ–∫–∞', `${macro.mortgageRate}%`],
    ['–°–µ–º–µ–π–Ω–∞—è –∏–ø–æ—Ç–µ–∫–∞', macro.familyMortgage ? '6%' : '–ù–µ—Ç'],
    ['–¢—Ä–µ–Ω–¥', macro.marketTrend]
  ];
  
  return reportData;
}

/**
 * [–£–õ–£–ß–®–ï–ù–û v1.0] –°–æ–∑–¥–∞–µ—Ç —Ç–µ–∫—Å—Ç–æ–≤—ã–π –æ—Ç—á–µ—Ç –ø–æ –æ–±—ä–µ–∫—Ç—É —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º –ò–ò –∞–Ω–∞–ª–∏—Ç–∏–∫–∞.
 * –¢–µ–ø–µ—Ä—å –∏—Å–ø–æ–ª—å–∑—É–µ—Ç callGeminiAnalyst –¥–ª—è —Å—Ç—Ä—É–∫—Ç—É—Ä–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ –∞–Ω–∞–ª–∏–∑–∞.
 * @param {Array} objectData –î–∞–Ω–Ω—ã–µ –æ–±—ä–µ–∫—Ç–∞.
 * @param {Object} analysisResults –†–µ–∑—É–ª—å—Ç–∞—Ç—ã –∞–Ω–∞–ª–∏–∑–∞.
 * @param {Object} competitorsData –î–∞–Ω–Ω—ã–µ –∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–æ–≤.
 * @returns {string} –¢–µ–∫—Å—Ç –æ—Ç—á–µ—Ç–∞.
 */
function createSingleObjectReportText(objectData, analysisResults, competitorsData) {
  const { activeCompetitors, soldCompetitors } = competitorsData;
  
  // –ü–æ–ª—É—á–∞–µ–º —Å–µ–∑–æ–Ω–Ω—ã–µ –∏ –º–∞–∫—Ä–æ-–¥–∞–Ω–Ω—ã–µ
  const currentMonth = new Date().getMonth() + 1;
  const seasonalCoef = SEASONAL_COEFFICIENTS[currentMonth] || 1.0;
  const macro = getMacroContext();
  const monthNames = ['', '–Ø–Ω–≤–∞—Ä—å', '–§–µ–≤—Ä–∞–ª—å', '–ú–∞—Ä—Ç', '–ê–ø—Ä–µ–ª—å', '–ú–∞–π', '–ò—é–Ω—å', 
                      '–ò—é–ª—å', '–ê–≤–≥—É—Å—Ç', '–°–µ–Ω—Ç—è–±—Ä—å', '–û–∫—Ç—è–±—Ä—å', '–ù–æ—è–±—Ä—å', '–î–µ–∫–∞–±—Ä—å'];
  
  // –†–∞—Å—á—ë—Ç –∫–∞—á–µ—Å—Ç–≤–∞ –æ–±—ä–µ–∫—Ç–∞
  const dataMap = {
    '–¢–∏–ø —Ä–µ–º–æ–Ω—Ç–∞': objectData[6],
    '–ì–æ–¥ –ø–æ—Å—Ç—Ä–æ–π–∫–∏': objectData[10],
    '–≠—Ç–∞–∂': objectData[8],
    '–≠—Ç–∞–∂–Ω–æ—Å—Ç—å': objectData[9],
    '–ø—Ä–æ—Ñ. —Ñ–æ—Ç–æ': objectData[11],
    '–æ–ø–∏—Å–∞–Ω–∏–µ': objectData[12],
    '–ø–ª–∞–Ω–∏—Ä–æ–≤–∫–∞': objectData[13]
  };
  const qualityScore = calculateQualityScore(dataMap);
  
  // –ü–æ–¥–≥–æ—Ç–∞–≤–ª–∏–≤–∞–µ–º —Å—ã—Ä—ã–µ –¥–∞–Ω–Ω—ã–µ –¥–ª—è –ò–ò-–∞–Ω–∞–ª–∏—Ç–∏–∫–∞
  const analyticsData = {
    objectInfo: {
      code: objectData[5],
      district: objectData[1],
      rooms: objectData[3],
      area: objectData[4],
      buildYear: objectData[10],
      repair: objectData[6],
      floor: objectData[8],
      totalFloors: objectData[9],
      price: Number(objectData[7]),
      pricePerSqm: Math.round(Number(objectData[7]) / Number(objectData[4]))
    },
    marketData: {
      assessment: analysisResults.marketAssessment,
      demandPerMonth: analysisResults.demandPerMonth,
      liquidityType: analysisResults.liquidityType,
      activeCount: activeCompetitors.length,
      soldCount: soldCompetitors.length,
      avgNewObjectsPerMonth: analysisResults.avgNewObjectsPerMonth,
      arrivalToDemandRatio: analysisResults.arrivalToDemandRatio
    },
    positioning: {
      rank: analysisResults.objectRank,
      activeRatio: analysisResults.marketRatios.activeRatio,
      soldRatio: analysisResults.marketRatios.soldRatio,
      avgActivePrice: analysisResults.marketRatios.avgActivePrice,
      avgSoldPrice: analysisResults.marketRatios.avgSoldPrice
    },
    forecasts: analysisResults.forecasts,
    factors: {
      qualityScore: qualityScore,
      seasonalCoef: seasonalCoef,
      currentMonth: monthNames[currentMonth]
    }
  };
  
  // –ü—Ä–æ–±—É–µ–º –≤—ã–∑–≤–∞—Ç—å –ò–ò-–∞–Ω–∞–ª–∏—Ç–∏–∫–∞
  const aiResult = callGeminiAnalyst(analyticsData, macro);
  
  // –ï—Å–ª–∏ –ò–ò-–∞–Ω–∞–ª–∏—Ç–∏–∫ –≤–µ—Ä–Ω—É–ª —Å—Ç—Ä—É–∫—Ç—É—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ
  if (aiResult.success) {
    const ai = aiResult;
    
    // –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º —Å—Ç—Ä—É–∫—Ç—É—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–π –æ—Ç–≤–µ—Ç –≤ —Ç–µ–∫—Å—Ç
    let report = '';
    
    // –ö—Ä–∞—Ç–∫–∞—è –æ—Ü–µ–Ω–∫–∞ —Ä—ã–Ω–∫–∞
    report += '–ö—Ä–∞—Ç–∫–∞—è –æ—Ü–µ–Ω–∫–∞ —Ä—ã–Ω–∫–∞.\n';
    report += ai.analysis?.marketPosition || `–ú—ã –ø—Ä–æ–∞–Ω–∞–ª–∏–∑–∏—Ä–æ–≤–∞–ª–∏ —Å–∏—Ç—É–∞—Ü–∏—é –≤ —Ä–∞–π–æ–Ω–µ "${objectData[1]}". `;
    report += ` –¢–µ–∫—É—â–∏–π —Å–ø—Ä–æ—Å –Ω–∞ –Ω–µ–¥–≤–∏–∂–∏–º–æ—Å—Ç—å –∑–¥–µ—Å—å –æ—Ü–µ–Ω–∏–≤–∞–µ—Ç—Å—è –∫–∞–∫ ${analysisResults.marketAssessment.toLowerCase()}, `;
    report += `–∞ —ë–º–∫–æ—Å—Ç—å —Ä—ã–Ω–∫–∞ —Å–æ—Å—Ç–∞–≤–ª—è–µ—Ç ${analysisResults.demandPerMonth.toFixed(1)} –æ–±—ä–µ–∫—Ç–∞ –≤ –º–µ—Å—è—Ü. `;
    report += `–£—Ä–æ–≤–µ–Ω—å –ª–∏–∫–≤–∏–¥–Ω–æ—Å—Ç–∏ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç —Ç–∏–ø—É ${analysisResults.liquidityType}. `;
    report += `–ü—Ä–∏ —ç—Ç–æ–º –∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–Ω–∞—è —Å—Ä–µ–¥–∞ ${analysisResults.arrivalToDemandRatio > 100 ? '–Ω–∞—Å—ã—â–µ–Ω–∞' : '—É–º–µ—Ä–µ–Ω–Ω–∞'}: `;
    report += `–≤ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç –≤ –ø—Ä–æ–¥–∞–∂–µ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è ${activeCompetitors.length} –∞–∫—Ç–∏–≤–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤, `;
    report += `—Å –∫–æ—Ç–æ—Ä—ã–º–∏ –≤–∞–º –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç —Å–æ–ø–µ—Ä–Ω–∏—á–∞—Ç—å –∑–∞ –ø–æ–∫—É–ø–∞—Ç–µ–ª—è.\n\n`;
    
    // –ü–æ–∑–∏—Ü–∏—è –æ–±—ä–µ–∫—Ç–∞
    report += '–ü–æ–∑–∏—Ü–∏—è –æ–±—ä–µ–∫—Ç–∞ –Ω–∞ —ç—Ç–æ–º —Ä—ã–Ω–∫–µ.\n';
    report += `–í–∞—à–∞ ${objectData[3]}-–∫–æ–º–Ω–∞—Ç–Ω–∞—è –∫–≤–∞—Ä—Ç–∏—Ä–∞ –ø–ª–æ—â–∞–¥—å—é ${objectData[4]} –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö –º–µ—Ç—Ä–æ–≤ –≤—ã—Å—Ç–∞–≤–ª–µ–Ω–∞ –ø–æ —Ü–µ–Ω–µ ${Number(objectData[7]).toLocaleString()} —Ä—É–±–ª–µ–π. `;
    report += `–û—Ç–Ω–æ—Å–∏—Ç–µ–ª—å–Ω–æ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–π –∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ —Å–µ–π—á–∞—Å –Ω–∞—Ö–æ–¥—è—Ç—Å—è –≤ —Ä–µ–∫–ª–∞–º–µ, –≤–∞—à–∞ —Å—Ç–æ–∏–º–æ—Å—Ç—å `;
    report += `${analysisResults.marketRatios.activeRatio > 0 ? '–≤—ã—à–µ' : '–Ω–∏–∂–µ'} —Å—Ä–µ–¥–Ω–µ–π –Ω–∞ ${Math.abs(analysisResults.marketRatios.activeRatio).toFixed(1)}%. `;
    report += `–û–¥–Ω–∞–∫–æ –ø—Ä–∏ —Å—Ä–∞–≤–Ω–µ–Ω–∏–∏ —Å —Ä–µ–∞–ª—å–Ω—ã–º–∏ —Å–¥–µ–ª–∫–∞–º–∏ —Ç–µ–∫—É—â–∞—è —Ü–µ–Ω–∞ –æ—Ç–ª–∏—á–∞–µ—Ç—Å—è –Ω–∞ ${Math.abs(analysisResults.marketRatios.soldRatio).toFixed(1)}% `;
    report += `–æ—Ç —Å—Ä–µ–¥–Ω–µ–π —Å—Ç–æ–∏–º–æ—Å—Ç–∏ —É–∂–µ –ø—Ä–æ–¥–∞–Ω–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤. `;
    
    // –î–æ–±–∞–≤–ª—è–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ —Å–µ–∑–æ–Ω–Ω–æ—Å—Ç–∏
    if (seasonalCoef !== 1.0) {
      report += `\n\n–í–∞–∂–Ω–æ —É—á–∏—Ç—ã–≤–∞—Ç—å —Å–µ–∑–æ–Ω–Ω—ã–π —Ñ–∞–∫—Ç–æ—Ä: ${monthNames[currentMonth]} `;
      report += seasonalCoef > 1 ? `—Ç—Ä–∞–¥–∏—Ü–∏–æ–Ω–Ω–æ —É–≤–µ–ª–∏—á–∏–≤–∞–µ—Ç —Å—Ä–æ–∫–∏ –ø—Ä–æ–¥–∞–∂–∏ –Ω–∞ ${Math.round((seasonalCoef - 1) * 100)}%.` : 
                                   `–±–ª–∞–≥–æ–ø—Ä–∏—è—Ç–µ–Ω –¥–ª—è –ø—Ä–æ–¥–∞–∂ ‚Äî —Å—Ä–æ–∫–∏ —Å–æ–∫—Ä–∞—â–∞—é—Ç—Å—è –Ω–∞ ${Math.round((1 - seasonalCoef) * 100)}%.`;
    }
    
    // –î–æ–±–∞–≤–ª—è–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –º–∞–∫—Ä–æ–∫–æ–Ω—Ç–µ–∫—Å—Ç–µ
    if (macro.keyRate >= 15) {
      report += ` –ü—Ä–∏ —Ç–µ–∫—É—â–µ–π –∫–ª—é—á–µ–≤–æ–π —Å—Ç–∞–≤–∫–µ –¶–ë ${macro.keyRate}% –¥–æ—Å—Ç—É–ø–Ω–æ—Å—Ç—å –∏–ø–æ—Ç–µ–∫–∏ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∞, —á—Ç–æ —Å—É–∂–∞–µ—Ç –∫—Ä—É–≥ –ø–æ–∫—É–ø–∞—Ç–µ–ª–µ–π.`;
    }
    if (macro.familyMortgage) {
      report += ` –î–ª—è —Å–µ–º–µ–π —Å –¥–µ—Ç—å–º–∏ –¥–æ—Å—Ç—É–ø–Ω–∞ –ª—å–≥–æ—Ç–Ω–∞—è –∏–ø–æ—Ç–µ–∫–∞ –ø–æ–¥ 6%, —á—Ç–æ –º–æ–∂–µ—Ç –ø—Ä–∏–≤–ª–µ—á—å —ç—Ç—É –∫–∞—Ç–µ–≥–æ—Ä–∏—é –ø–æ–∫—É–ø–∞—Ç–µ–ª–µ–π.`;
    }
    report += '\n\n';
    
    // –¶–µ–Ω–æ–≤—ã–µ —Å—Ç—Ä–∞—Ç–µ–≥–∏–∏
    report += '–¶–µ–Ω–æ–≤—ã–µ —Å—Ç—Ä–∞—Ç–µ–≥–∏–∏ —Å –ø—Ä–æ–≥–Ω–æ–∑–∞–º–∏.\n';
    report += `–û–ø–∏—Ä–∞—è—Å—å –Ω–∞ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É, –º—ã —Ä–∞—Å—Å—á–∏—Ç–∞–ª–∏ –¥–≤–∞ –≤–æ–∑–º–æ–∂–Ω—ã—Ö —Å—Ü–µ–Ω–∞—Ä–∏—è —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏. `;
    report += `–°—Ç—Ä–∞—Ç–µ–≥–∏—è –±—ã—Å—Ç—Ä–æ–π –ø—Ä–æ–¥–∞–∂–∏ –ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ—Ç —Ü–µ–Ω—É –æ–∫–æ–ª–æ ${analysisResults.forecasts.quickPrice.toLocaleString()} —Ä—É–±–ª–µ–π `;
    report += `—Å –ø—Ä–æ–≥–Ω–æ–∑–æ–º –≤—ã—Ö–æ–¥–∞ –Ω–∞ —Å–¥–µ–ª–∫—É –∑–∞ ${analysisResults.forecasts.quickMonths} –º–µ—Å. `;
    report += `–°—Ç—Ä–∞—Ç–µ–≥–∏—è —Ä—ã–Ω–æ—á–Ω–æ–π –ø—Ä–æ–¥–∞–∂–∏ –æ—Ä–∏–µ–Ω—Ç–∏—Ä–æ–≤–∞–Ω–∞ –Ω–∞ —Å—Ç–æ–∏–º–æ—Å—Ç—å ${analysisResults.forecasts.marketPrice.toLocaleString()} —Ä—É–±–ª–µ–π, `;
    report += `–ø—Ä–∏ —ç—Ç–æ–º –æ–∂–∏–¥–∞–µ–º—ã–π —Å—Ä–æ–∫ —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ —Ç–∞–∫–∂–µ —Å–æ—Å—Ç–∞–≤–ª—è–µ—Ç ${analysisResults.forecasts.marketMonths} –º–µ—Å.`;
    
    // –î–æ–±–∞–≤–ª—è–µ–º —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –ò–ò
    if (ai.recommendations && ai.recommendations.length > 0) {
      report += '\n\n–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: ';
      ai.recommendations.slice(0, 3).forEach((rec, i) => {
        report += `${i + 1}) ${rec.action}${rec.impact ? ` (${rec.impact})` : ''}. `;
      });
    }
    
    // –£—Ä–æ–≤–µ–Ω—å —É–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç–∏
    if (ai.confidence) {
      report += `\n\n–£—Ä–æ–≤–µ–Ω—å —É–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç–∏ –≤ –ø—Ä–æ–≥–Ω–æ–∑–µ: ${ai.confidence}%.`;
      if (ai.confidenceReasoning) {
        report += ` ${ai.confidenceReasoning}`;
      }
    }
    
    return report;
  }
  
  // –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω—ã–µ –æ–ø–∏—Å–∞–Ω–∏—è –¥–ª—è "–ø—Ä–æ—Å—Ç—ã—Ö —Å–º–µ—Ä—Ç–Ω—ã—Ö"
  const liquidityDescriptions = {
    'A+': '–æ—á–µ–Ω—å –≤—ã—Å–æ–∫–∞—è (–ø—Ä–æ–¥–∞–∂–∞ –∑–∞ —Å—á–∏—Ç–∞–Ω–Ω—ã–µ –¥–Ω–∏)',
    'A': '–≤—ã—Å–æ–∫–∞—è (–±—ã—Å—Ç—Ä–∞—è –ø—Ä–æ–¥–∞–∂–∞)',
    'B': '—Å—Ä–µ–¥–Ω—è—è (—Å–±–∞–ª–∞–Ω—Å–∏—Ä–æ–≤–∞–Ω–Ω—ã–π —Å–ø—Ä–æ—Å)',
    'C': '–Ω–∏–∑–∫–∞—è (–¥–æ–ª–≥–∞—è –ø—Ä–æ–¥–∞–∂–∞)',
    'C-': '–æ—á–µ–Ω—å –Ω–∏–∑–∫–∞—è (—Ä—ã–Ω–æ–∫ "—Å—Ç–æ–∏—Ç")'
  };
  const liquidityText = liquidityDescriptions[analysisResults.liquidityType] || analysisResults.liquidityType;

  // Fallback –Ω–∞ —Å—Ç–∞—Ä—ã–π –º–µ—Ç–æ–¥, –µ—Å–ª–∏ –ò–ò-–∞–Ω–∞–ª–∏—Ç–∏–∫ –Ω–µ —Å—Ä–∞–±–æ—Ç–∞–ª
  const geminiSummary = `–û–ë–™–ï–ö–¢: ${objectData[3]}-–∫–æ–º–Ω–∞—Ç–Ω–∞—è –∫–≤–∞—Ä—Ç–∏—Ä–∞ ${objectData[4]} –º¬≤ –≤ —Ä–∞–π–æ–Ω–µ "${objectData[1]}".
–û–¶–ï–ù–ö–ê –†–´–ù–ö–ê: ${analysisResults.marketAssessment}. –í —Ä–∞–π–æ–Ω–µ –ø—Ä–æ–¥–∞—ë—Ç—Å—è –ø—Ä–∏–º–µ—Ä–Ω–æ ${analysisResults.demandPerMonth.toFixed(1)} –∞–Ω–∞–ª–æ–≥–∏—á–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤ –≤ –º–µ—Å—è—Ü.
–°–ö–û–†–û–°–¢–¨ –ü–†–û–î–ê–ñ–ò (–õ–∏–∫–≤–∏–¥–Ω–æ—Å—Ç—å): ${liquidityText}.
–ö–û–ù–ö–£–†–ï–ù–¶–ò–Ø: –°–µ–π—á–∞—Å –≤ –ø—Ä–æ–¥–∞–∂–µ ${activeCompetitors.length} –æ–±—ä–µ–∫—Ç–æ–≤-–∫–æ–Ω–∫—É—Ä–µ–Ω—Ç–æ–≤.
–°–ï–ó–û–ù–ù–û–°–¢–¨: ${monthNames[currentMonth]}, —Ñ–∞–∫—Ç–æ—Ä —Å–µ–∑–æ–Ω–∞: ${seasonalCoef > 1 ? '–∑–∞–º–µ–¥–ª—è–µ—Ç' : '—É—Å–∫–æ—Ä—è–µ—Ç'} –ø—Ä–æ–¥–∞–∂—É –Ω–∞ ${Math.abs(Math.round((seasonalCoef - 1) * 100))}%.
