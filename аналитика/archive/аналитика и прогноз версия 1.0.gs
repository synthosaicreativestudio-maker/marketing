function setAdditionalHeaders() {
    // –ò–°–ü–†–ê–í–õ–ï–ù–ò–ï: –ò—Å–ø—Ä–∞–≤–ª–µ–Ω–∞ TypeError: SpreadsheetApp.getSheetByName –Ω–∞ SPREADSHEET.getSheetByName
    const sheet = SPREADSHEET.getSheetByName(SHEETS.MAIN_ANALYTICS);
